{% for post in site.posts %}
  <div>
    <h2><a href="{{ post.url }}">{{ post.title }}</a></h2>
    <span><small style="font-size: x-small">{{ post.date | date_to_string }}</small></span>
    <hr />
    <div>
      {{ post.content }}
    </div>
  </div>
  <hr />
{% endfor %}


